{
  "name": "neocities",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --env-file .env --import @greenwood/cli/register ./node_modules/@greenwood/cli/src/bin.js develop",
    "prebuild": "rm -rf ./public/ && pnpm run generate-thumbs",
    "build": "node --env-file .env --import @greenwood/cli/register ./node_modules/@greenwood/cli/src/bin.js build",
    "serve": "node --env-file .env --import @greenwood/cli/register ./node_modules/@greenwood/cli/src/bin.js serve",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-thumbs": "node scripts/generate-thumbs.ts",
    "lint": "eslint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@greenwood/cli": "0.34.0-alpha.1",
    "@greenwood/plugin-import-raw": "0.34.0-alpha.1",
    "@greenwood/plugin-markdown": "0.34.0-alpha.1",
    "@greenwood/plugin-postcss": "0.34.0-alpha.1",
    "@greenwood/plugin-renderer-lit": "0.34.0-alpha.1",
    "@mapbox/rehype-prism": "^0.9.0",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/web-components-vite": "^10.0.2",
    "@types/node": "^24.10.0",
    "@types/trusted-types": "^2.0.7",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/browser-playwright": "^4.0.4",
    "@vitest/expect": "^4.0.4",
    "csstype": "^3.1.3",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-storybook": "^10.0.2",
    "eslint-plugin-vitest": "^0.5.4",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "material-colors": "^1.2.6",
    "mix-n-matchers": "^2.0.1",
    "msw": "^2.11.6",
    "msw-storybook-addon": "^2.0.6",
    "normalize.css": "^8.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-extend-rule": "^4.0.0",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prism-theme-github": "^1.0.2",
    "sharp": "^0.34.4",
    "storybook": "^10.0.2",
    "ts-lit-plugin": "^2.0.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.4",
    "vitest-browser-lit": "^1.0.1"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@lit/task": "^1.0.3",
    "@open-wc/dedupe-mixin": "^2.0.1",
    "@tanstack/pacer": "^0.16.0",
    "@tanstack/query-core": "^5.90.5",
    "dracula-prism": "^2.1.16",
    "ky": "^1.13.0",
    "lit": "^3.3.1",
    "nanoid": "^5.1.6",
    "rad-event-listeners": "^1.0.1",
    "valibot": "^1.1.0"
  },
  "engines": {
    "node": ">=24.0.0"
